<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Hitelesítő adatok tárolója</string>

    <!-- Dialog title when no certificates were found -->
    <string name="title_no_certs">Nem található tanusítvány</string>

    <!-- Dialog title when at least one certificate was found -->
    <string name="title_select_cert">Válasszon tanúsítványt</string>

    <!-- Used at top of dialog to identify requesting application -->
    <string name="requesting_application">A(z) %s alkalmazás tanúsítványt kér. Válassza ki azt tanúsítványt, amit a jövőben azonosításra használhat.</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <string name="requesting_server">Kérelmező szerver: %s. Csak akkor bíztosítson hozzáférést a tanusítványhoz, ha megbízik az alkalmazásban.</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <string name="install_new_cert_message">A külső memóriakártyáról %1$s vagy %2$s kiterjesztésű PKCS#12 tanusítvány telepíthető.</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <string name="install_new_cert_button_label">Telepítés</string>

    <!-- Label of button to send the allow the application certificate request -->
    <string name="allow_button">Engedélyez</string>

    <!-- Label of button to send the deny the application certificate request -->
    <string name="deny_button">Megtagad</string>

</resources>
